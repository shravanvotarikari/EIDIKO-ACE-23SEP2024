{
  "$schema": "http://json-schema.org/draft-04/schema#",
   "type": "object",
    "properties": {
    "bankID": {
      "type": "string",
      "pattern": "^[0-9]+$",
      "maxLength": 3
    },
    "mobileNumber": {
      "type": "string",
      "pattern": "^[0-9]{10}$"
    },
    "accountNumber": {
      "type": "string",
      "pattern": "^[0-9]+$",
      "minLength": 12,
      "maxLength": 12
    },
    "token": {
      "type": "string",
      "pattern": "^[0-9]{6}$"
    },
    "reference_id": {
      "type": "string",
      "pattern": "^[A-Za-z0-9-]+$"
    }
  },
   "required": [ "mobileNumber", "accountNumber", "token", "reference_id"],
  "additionalProperties": false
 
}
