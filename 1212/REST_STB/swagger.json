{
  "swagger" : "2.0",
  "info" : {
    "title" : "Rest_STB",
    "version" : "1.0.0",
    "description" : "Rest_STB"
  },
  "paths" : {
    "/validate" : {
      "post" : {
        "operationId" : "postValidate",
        "responses" : {
          "200" : {
            "description" : "The operation was successful."
          }
        },
        "consumes" : [ "application/json" ],
        "produces" : [ "application/json" ],
        "description" : "Insert a validate",
        "parameters" : [ {
          "name" : "body",
          "in" : "body",
          "schema" : {
            "type" : "string"
          },
          "description" : "The request body for the operation",
          "required" : true
        } ]
      }
    }
  },
  "basePath" : "/rest_stb/v1",
  "definitions" : {
    "parameters" : {
      "type" : "object",
      "properties" : {
        "clientTransactionId" : {
          "type" : "string",
          "pattern" : "^[A-Za-z0-9]{6,25}$"
        },
        "transactionType" : {
          "type" : "string",
          "pattern":"^(CASH_ADVANCE|ADVANCE_PAYMENT)$"
        
        },
        "requestDate" : {
          "type" : "string",
          "pattern": "^\\d{8}$"
        },
        "creditAccount" : {
          "type" : "string",
          "pattern" :"^\\d{12,16}$"
        },
        "creditCardAccount" : {
          "type" : "string",
           "pattern" :"^\\d{16}$"
        },
        "amount" : {
          "type" : "string",
           "pattern" :"^\\d+(\\.\\d{1,2})?$"
        },
        "channel" : {
          "type" : "string",
           "pattern" :"^1$"
        },
        "description" : {
          "type" : "string",
           "pattern" :"^[\\w\\s]+$"
        },
        "username" : {
          "type" : "string",
           "pattern" :"^[A-Za-z0-9]+$"
        }
      },
      "required" : [ "creditAccount", "creditCardAccount", "amount", "channel", "description", "username", "requestDate", "clientTransactionId", "transactionType" ]
    }
  }
}