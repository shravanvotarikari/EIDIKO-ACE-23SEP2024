

CREATE COMPUTE MODULE MF_Channel_Request_Compute1
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		
		DECLARE i INTEGER;
		SET i=CARDINALITY(Environment.[]);
		SET i=i+1;
		
    	DECLARE payload CHARACTER;
    	SET payload = CAST(ASBITSTREAM(InputRoot.JSON.Data) AS CHARACTER CCSID 1208 ENCODING 546);
    	
    	SET Environment.payload[i]=payload;
    	

    RETURN TRUE;
	END;
END MODULE;
