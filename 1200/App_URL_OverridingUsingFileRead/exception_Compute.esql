

CREATE COMPUTE MODULE exception_Compute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		 CALL CopyEntireMessage();
		RETURN TRUE;
	END;


	CREATE PROCEDURE CopyEntireMessage() BEGIN
		SET OutputRoot = InputRoot;
		SET Environment.NewData.APPNAME = InputRoot.XMLNSC.PropertiesInfo.Global.ApplicationName;
		SET Environment.NewData.newURL = InputRoot.XMLNSC.PropertiesInfo.Global.ESBURL;
	END;
END MODULE;
