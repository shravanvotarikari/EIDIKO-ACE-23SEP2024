

CREATE COMPUTE MODULE MF_Timer_Notification_Compute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		CALL CopyEntireMessage();
		RETURN TRUE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		SET OutputLocalEnvironment.date = CAST(CURRENT_TIMESTAMP AS CHARACTER);
		INSERT INTO Database.TIMERNOTIFICATIONDATA VALUES (OutputLocalEnvironment.date);
	END;
END MODULE;
