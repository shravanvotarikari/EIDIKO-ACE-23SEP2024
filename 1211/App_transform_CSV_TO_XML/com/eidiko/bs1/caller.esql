BROKER SCHEMA com.eidiko.bs1

CREATE PROCEDURE intgerPROC(IN a INTEGER, IN b INTEGER, OUT sum INTEGER)
BEGIN
	
	SET sum = CAST( CAST(a AS CHARACTER) || CAST(b AS CHARACTER)AS INTEGER);
END;

CREATE PROCEDURE charPROC(IN firstName CHARACTER, IN lastName CHARACTER, OUT fullName CHARACTER)
BEGIN
	SET fullName = firstName || ' ' || lastName;
END;