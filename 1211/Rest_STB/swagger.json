        "clientTransactionId" : {
          "type" : "string",
          "pattern" : "^[A-Za-z0-9]{6,25}$"
        },
        "transactionType" : {
          "type" : "string",
          "pattern" : "^(CASH_ADVANCE|ADVANCE_PAYMENT)$"
        },
        "requestDate" : {
          "type" : "string",
          "format" : "date"
        },
        "creditAccount" : {
          "type" : "string",
          "pattern" : "^\\d{12,16}$"
        },
        "creditCardAccount" : {
          "type" : "string",
          "pattern" : "^\\d{12,16}$"
        },
        "amount" : {
          "type" : "number",
          "pattern" : "^\\d+$"
        },
        "channel" : {
          "type" : "string",
          "pattern" : "^\\d{1}$"
        },
        "description" : {
          "type" : "string",
          "pattern" : "^[A-Za-z0-9]{12,16}$"
        },
        "username" : {
          "type" : "string",
          "pattern" : "^[A-Za-z]+$"
        }
      },
      "required" : [ "clientTransactionId", "transactionType", "requestDate", "creditAccount", "creditCardAccount", "amount", "channel", "description", "username" ]
    },
