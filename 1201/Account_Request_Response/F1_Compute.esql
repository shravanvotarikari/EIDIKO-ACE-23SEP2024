

CREATE COMPUTE MODULE F1_Compute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		
	    CALL CopyEntireMessage();
		RETURN TRUE;
	END;

	

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		
		set OutputRoot = InputRoot;
		
		DECLARE check CHARACTER;
		SET check = InputRoot.XMLNSC.a.request.accountOpeningCheck;
		
		IF check = 'KYC_CHECK' THEN
			
			SET OutputRoot.XMLNSC.a.Data.S1 = 'successfull';
			
		ELSE
			
			Propagate TO TERMINAL 'Failure';
			
		END IF;

	END;
END MODULE;
